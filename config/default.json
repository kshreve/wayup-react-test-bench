{
    "env": "default",
    "host": "localhost",
    "port": "3001",
    "publicPath": "/",
    "assetPath": "assets",
    "jsSourcePath": "src/js",
    "optimization": {
        "analyzeMode": false,
        "analyze": {
            "port": 8888
        }
    },
    "browserSync": {
        "active": true,
        "port": 3002
    },
    "html": [
        {
            "name": "homepage",
            "title": "TestBench | WayUp",
            "template": "_default.html",
            "filename": "index.html"
        }
    ],
    "app": {
        "description": "config inside 'app' will be injected to your frontend code automatically"
    }
}
